<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<watcherConfig>

	<watch>
		<directories>
			<dir>${sys:user.home}/Downloads</dir>
		</directories>
		<initialScan>true</initialScan>
		<rejectAlreadyProcessedFiles>true</rejectAlreadyProcessedFiles>
	</watch>

	<parsing>
		<filenamePatterns>*.srt</filenamePatterns>
		<parsers>
			<parser enabled="true">addic7ed.com</parser>
			<parser enabled="false">italiansubs.net</parser>
			<parser enabled="false">subcentral.de</parser>
		</parsers>
	</parsing>

	<metadata>
		<release>
			<parsers>
				<parser enabled="true">release.scene</parser>
			</parsers>
			<metaTags>
				<tag>REAL</tag>
				<tag>PROPER</tag>
				<tag>REPACK</tag>
				<tag>DiRFIX</tag>
				<tag>NFOFIX</tag>
				<tag>READNFO</tag>
				<tag>iNTERNAL</tag>
			</metaTags>
			<databases>
				<db enabled="true">predb.me</db>
				<db enabled="true">xrel.to</db>
				<db enabled="false">orlydb.com</db>
			</databases>
			<guessing enabled="true">
				<standardReleases>
					<standardRelease tags="1080p, WEB-DL, DD5.1, H.264" scope="IF_GUESSING" />
					<standardRelease tags="720p, WEB-DL, DD5.1, H.264" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="2HD" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="2HD" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, XviD" group="AFG" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="ASAP" scope="IF_GUESSING" />
					<standardRelease tags="1080p, HDTV, x264" group="BATV" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="BATV" scope="IF_GUESSING" />
					<standardRelease tags="1080i, HDTV, MPEG2, DD5.1" group="CtrlHD" scope="IF_GUESSING" />
					<standardRelease tags="1080p, HDTV, x264" group="DIMENSION" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="DIMENSION" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="DON" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, XviD" group="FUM" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV x264" group="IMMERSE" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="LOL" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="KILLERS" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="KILLERS" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="EXCELLENCE" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="REMARKABLE" scope="IF_GUESSING" />
					<standardRelease tags="720p, HDTV, x264" group="FoV" scope="IF_GUESSING" />
					<standardRelease tags="HDTV, x264" group="FoV" scope="IF_GUESSING" />
				</standardReleases>
			</guessing>
			<compatibility enabled="true">
				<crossGroupCompatibilityRules>
					<crossGroupCompatibilityRule enabled="true" sourceGroup="CtrlHD" compatibleGroup="DON" symmetric="true" />
					<crossGroupCompatibilityRule enabled="true" sourceGroup="DIMENSION" compatibleGroup="LOL" symmetric="true" />
					<crossGroupCompatibilityRule enabled="true" sourceGroup="IMMERSE" compatibleGroup="ASAP" symmetric="true" />
					<crossGroupCompatibilityRule enabled="true" sourceGroup="REMARKABLE" compatibleGroup="EXCELLENCE" symmetric="true" />
				</crossGroupCompatibilityRules>
			</compatibility>
		</release>
	</metadata>

	<correction>
		<rules>
			<seriesNameCorrectionRule namePattern="Castle" namePatternMode="LITERAL" nameReplacement="Castle (2009)" beforeQuerying="true" afterQuerying="false" />
			<seriesNameCorrectionRule namePattern="Childrens Hospital" namePatternMode="LITERAL" nameReplacement="Childrens Hospital" beforeQuerying="true" afterQuerying="false">
				<aliasNames>
					<aliasName>Childrens Hospital (US)</aliasName>
				</aliasNames>
			</seriesNameCorrectionRule>
			<seriesNameCorrectionRule namePattern="Doctor Who" namePatternMode="LITERAL" nameReplacement="Doctor Who (2005)" beforeQuerying="true" afterQuerying="false" />
			<seriesNameCorrectionRule namePattern="From Dusk Till Dawn*The Series" namePatternMode="SIMPLE" nameReplacement="From Dusk Till Dawn: The Series" beforeQuerying="true" afterQuerying="false">
				<aliasNames>
					<aliasName>From Dusk Till Dawn</aliasName>
				</aliasNames>
			</seriesNameCorrectionRule>
			<seriesNameCorrectionRule namePattern="Last Man Standing" namePatternMode="LITERAL" nameReplacement="Last Man Standing (US)" beforeQuerying="true" afterQuerying="false" />
			<seriesNameCorrectionRule namePattern="Nashville" namePatternMode="LITERAL" nameReplacement="Nashville (2012)" beforeQuerying="true" afterQuerying="false">
				<aliasNames>
					<aliasName>Nashville</aliasName>
				</aliasNames>
			</seriesNameCorrectionRule>
			<seriesNameCorrectionRule namePattern="Scandal" namePatternMode="LITERAL" nameReplacement="Scandal (US)" beforeQuerying="true" afterQuerying="false" />
			<seriesNameCorrectionRule namePattern="Scream" namePatternMode="LITERAL" nameReplacement="Scream The TV Series" beforeQuerying="true" afterQuerying="false">
				<aliasNames>
					<aliasName>Scream (2015)</aliasName>
					<aliasName>Scream</aliasName>
				</aliasNames>
			</seriesNameCorrectionRule>
			<seriesNameCorrectionRule namePattern="Scream Queens" namePatternMode="LITERAL" nameReplacement="Scream Queens (2015)" beforeQuerying="true" afterQuerying="false" />
			<seriesNameCorrectionRule namePattern="The Magicians (2016)" namePatternMode="LITERAL" nameReplacement="The Magicians (US)" beforeQuerying="true" afterQuerying="false">
				<aliasNames>
					<aliasName>The Magicians</aliasName>
				</aliasNames>
			</seriesNameCorrectionRule>
			<seriesNameCorrectionRule namePattern="You're the Worst (2014)" namePatternMode="LITERAL" nameReplacement="You're the Worst" beforeQuerying="true" afterQuerying="false" />

			<releaseTagsCorrectionRule searchTags="AAC2, 0" replacement="AAC2.0" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="DD5, 1" replacement="DD5.1" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="H, 264" replacement="H.264" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="H, 265" replacement="H.265" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="H264" replacement="H.264" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="H265" replacement="H.265" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="x, 264" replacement="x264" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
			<releaseTagsCorrectionRule searchTags="x, 265" replacement="x265" searchMode="CONTAIN" replaceMode="MATCHED_SEQUENCE" ignoreOrder="false" beforeQuerying="true" afterQuerying="true" />
		</rules>

		<subtitleLanguage>
			<parsingLanguages>
				<language tag="en" />
				<language tag="de" />
			</parsingLanguages>
			<outputLanguageFormat>ISO2</outputLanguageFormat>
			<outputLanguage tag="en" />
			<customLanguagePatterns>
				<languagePattern pattern="VO" patternMode="LITERAL" languageTag="en" />
				<languagePattern pattern="VF" patternMode="LITERAL" languageTag="fr" />
				<languagePattern pattern="ger" patternMode="LITERAL" languageTag="de" />
				<languagePattern pattern="简体" patternMode="LITERAL" languageTag="zh-CN" /> <!-- simplified chinese -->
				<languagePattern pattern="繁体" patternMode="LITERAL" languageTag="zh-TW" /> <!-- traditional chinese -->
			</customLanguagePatterns>
			<customLanguageTextMappings>
				<languageTextMapping languageTag="en" text="VO" />
			</customLanguageTextMappings>
		</subtitleLanguage>
	</correction>

	<naming>
		<parameters>
			<param key="de.subcentral.core.name.EpisodeNamer.alwaysIncludeTitle" value="false" />
 			<param key="de.subcentral.core.name.ReleaseNamer.preferName" value="true" />
 			<param key="de.subcentral.core.name.SubtitleNamer.includeGroup" value="true" />
 			<param key="de.subcentral.core.name.SubtitleNamer.includeSource" value="false" />
		</parameters>
	</naming>

	<fileTransformation>
		<targetDir>${sys:user.home}/Downloads/!VO</targetDir>
		<deleteSource>false</deleteSource>
		<packing enabled="true">
			<sourceDeletionMode>DELETE</sourceDeletionMode>
			<winrar>
				<locateStrategy>AUTO_LOCATE</locateStrategy>
				<rarExe></rarExe>
			</winrar>
		</packing>
	</fileTransformation>

	<ui>
		<warnings enabled="true">
			<guessingWarning enabled="true" />
			<releaseMetaTaggedWarning enabled="true" />
			<releaseNukedWarning enabled="true" />
		</warnings>
		<systemTray enabled="true" />
	</ui>

</watcherConfig>