<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="standardizingSettingsPane" hgap="5.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="300.0" minWidth="500.0" vgap="5.0"
	xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="1.7976931348623157E308" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="%standardizing_rules_headline" underline="true">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Label>
		<Label text="%standardizing_rules_label" wrapText="true" GridPane.rowIndex="1" />
		<TableView fx:id="rulesTableView" editable="true" minHeight="100.0" prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="2">
			<columns>
				<TableColumn fx:id="rulesEnabledColumn" prefWidth="60.0" text="Enabled" />
				<TableColumn fx:id="rulesTypeColumn" prefWidth="100.0" text="Rule type" />
				<TableColumn fx:id="rulesOperationColumn" editable="false" prefWidth="450.0" text="Rule" />
			</columns>
		</TableView>
		<HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" GridPane.rowIndex="3">
			<children>
				<HBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" HBox.hgrow="SOMETIMES">
					<children>
						<Label maxHeight="1.7976931348623157E308" maxWidth="-Infinity" text="Rule type:" />
						<ChoiceBox fx:id="ruleTypeChoiceBox" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="100.0" prefWidth="150.0" />
						<Button fx:id="addRuleButton" mnemonicParsing="false" text="Add" />
					</children>
				</HBox>
				<Button fx:id="editRuleButton" mnemonicParsing="false" text="Edit" />
				<Button fx:id="removeRuleButton" mnemonicParsing="false" text="Remove" />
			</children>
		</HBox>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
