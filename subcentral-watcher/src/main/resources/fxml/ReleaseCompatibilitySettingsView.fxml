<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="rootPane" hgap="5.0" prefWidth="400.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" valignment="BOTTOM" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" valignment="BOTTOM" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="1.7976931348623157E308" minHeight="-Infinity" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Release compatibility" GridPane.columnSpan="2147483647">
			<graphic>
				<ImageView pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../img/couple_32.png" />
					</image>
				</ImageView>
			</graphic>
         <font>
            <Font size="18.0" />
         </font>
		</Label>
      <Separator prefWidth="200.0" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="10.0" />
         </GridPane.margin>
      </Separator>
		<CheckBox fx:id="compatibilityEnabledCheckBox" mnemonicParsing="false" text="Check for compatible releases" GridPane.rowIndex="2" />
		<Label text="Cross-group compatibilities" GridPane.columnSpan="2147483647" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" />
         </font>
         <GridPane.margin>
            <Insets top="25.0" />
         </GridPane.margin></Label>
		<Label disable="true" text="Releases of the same group are always compatible." GridPane.rowIndex="4" />
		<TableView fx:id="crossGroupCompatibilitiesTableView" editable="true" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="5">
			<columns>
				<TableColumn fx:id="crossGroupCompatibilitiesEnabledColumn" prefWidth="60.0" text="Enabled" />
				<TableColumn fx:id="crossGroupCompatibilitiesCompatibilityColumn" prefWidth="300.0" text="Cross-group compatibility" />
			</columns>
		</TableView>
      <HBox spacing="5.0" GridPane.columnSpan="2147483647" GridPane.rowIndex="6">
         <children>
				<Button fx:id="addCrossGroupCompatibilityBtn" mnemonicParsing="false" text="Add">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/add_16.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
				<Button fx:id="editCrossGroupCompatibilityBtn" mnemonicParsing="false" text="Edit">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/edit_16.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
				<Button fx:id="removeCrossGroupCompatibilityBtn" mnemonicParsing="false" text="Remove">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/remove_16.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
      </HBox>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
