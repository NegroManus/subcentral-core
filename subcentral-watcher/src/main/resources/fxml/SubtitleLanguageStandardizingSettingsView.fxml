<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="subLangStandardizingSettingsPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="Parsing">
               <content>
                  <GridPane hgap="5.0" vgap="5.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="100.0" />
                        <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="100.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Automatic parsing" underline="true" GridPane.columnSpan="2147483647" />
                        <Label text="Expect languages names to be written in one of these languages:" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
                        <TextField fx:id="parsingLangsTxtFld" editable="false" GridPane.rowIndex="2" />
                        <Button fx:id="editParsingLangsBtn" mnemonicParsing="false" text="Edit" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="Custom parsing" underline="true" GridPane.columnSpan="2147483647" GridPane.rowIndex="3" />
                        <TableView fx:id="langPatternsTableView" prefHeight="75.0" GridPane.rowIndex="4">
                          <columns>
                            <TableColumn fx:id="langPatternsPatternColumn" prefWidth="75.0" text="Pattern" />
                            <TableColumn fx:id="langPatternsLangColumn" prefWidth="75.0" text="Language" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <VBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <Button fx:id="addLangPatternBtn" mnemonicParsing="false" text="Add" />
                              <Button fx:id="editLangPatternBtn" mnemonicParsing="false" text="Edit" />
                              <Button fx:id="removeLangPatternBtn" mnemonicParsing="false" text="Remove" />
                              <Button fx:id="moveUpLangPatternBtn" mnemonicParsing="false" text="Move up">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="moveDownLangPatternBtn" mnemonicParsing="false" text="Move down" />
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </GridPane>
               </content>
            </Tab>
            <Tab text="Formatting">
               <content>
                  <GridPane hgap="5.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="100.0" />
                        <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity" />
                    </columnConstraints>
                     <children>
                        <Label text="Automatic formatting" underline="true" GridPane.columnSpan="2147483647" />
                        <Label text="Format" GridPane.rowIndex="1" />
                        <ChoiceBox fx:id="outputLangFormatChoiceBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label text="Format language" GridPane.rowIndex="2" />
                        <ComboBox fx:id="outputLangComboBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="Custom formatting" underline="true" GridPane.columnSpan="2147483647" GridPane.rowIndex="3" />
                        <TableView fx:id="langNamesTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="100.0" GridPane.columnSpan="2" GridPane.rowIndex="4">
                          <columns>
                            <TableColumn fx:id="langNamesLangColumn" prefWidth="75.0" text="Language" />
                            <TableColumn fx:id="langNamesNameColumn" prefWidth="75.0" text="Name" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <VBox spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                           <children>
                              <Button fx:id="addLangNameBtn" mnemonicParsing="false" text="Add" />
                              <Button fx:id="editLangNameBtn" mnemonicParsing="false" text="Edit" />
                              <Button fx:id="removeLangNameBtn" mnemonicParsing="false" text="Remove" />
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <rowConstraints>
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" />
                        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefHeight="200.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </center>
   <bottom>
      <GridPane hgap="5.0" vgap="5.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
          <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
          <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
          <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Testing:" underline="true" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label text="Input language" GridPane.rowIndex="2" />
            <Label text="Resulting output language" GridPane.rowIndex="3" />
            <TextField fx:id="testingInputTxtFld" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="testingOutputTxtFld" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Separator prefWidth="200.0" GridPane.columnSpan="2147483647" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </GridPane>
   </bottom>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <top>
      <Label text="Subtitle language standardizing" underline="true" BorderPane.alignment="CENTER_LEFT">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="5.0" />
         </BorderPane.margin>
      </Label>
   </top>
</BorderPane>
