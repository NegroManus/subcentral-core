<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="rootPane" hgap="5.0" prefWidth="500.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Release databases" underline="true" GridPane.columnSpan="2147483647">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../img/database_16.png" />
					</image>
				</ImageView>
			</graphic>
		</Label>
		<Label text="Release databases (ordered by priority):" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
		<TableView fx:id="releaseDbsTableView" editable="true" prefHeight="200.0" GridPane.rowIndex="2">
			<columns>
				<TableColumn fx:id="releaseDbsEnabledColumn" prefWidth="60.0" text="Enabled" />
				<TableColumn fx:id="releaseDbsNameColumn" editable="false" prefWidth="250.0" text="Release database" />
				<TableColumn fx:id="releaseDbsAvailableColumn" editable="false" prefWidth="75.0" text="Available" />
			</columns>
		</TableView>
		<VBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
			<children>
				<Button fx:id="moveUpReleaseDbButton" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Move up">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/up_16.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
				<Button fx:id="moveDownReleaseDbButton" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Move down">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/down_16.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
			</children>
		</VBox>
		<Button fx:id="recheckAvailabilitiesButton" mnemonicParsing="false" text="Recheck availabilities" GridPane.rowIndex="3">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/refresh_16.png" />
               </image>
            </ImageView>
         </graphic></Button>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
