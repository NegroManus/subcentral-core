<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<SplitPane dividerPositions="0.2" BorderPane.alignment="CENTER">
			<items>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0"
					prefWidth="100.0">
					<children>
						<TreeView fx:id="sectionSelectionTreeView" fixedCellSize="24.0"
							layoutX="-44.0" layoutY="263.0" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" showRoot="false"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
				<AnchorPane fx:id="sectionRootPane" minHeight="0.0"
					minWidth="0.0" prefHeight="500.0" prefWidth="500.0" />
			</items>
		</SplitPane>
	</center>
	<bottom>
		<HBox spacing="5.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="saveBtn" mnemonicParsing="false" text="Save">
					<graphic>
						<ImageView pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../img/download_16.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				<HBox HBox.hgrow="SOMETIMES" />
				<Button fx:id="restoreLastSavedBtn" mnemonicParsing="false"
					text="Restore last saved">
					<graphic>
						<ImageView pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../img/upload_16.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				<Button fx:id="restoreDefaultsBtn" mnemonicParsing="false"
					text="Restore defaults">
					<graphic>
						<ImageView pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../img/upload_16.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
	</bottom>
</BorderPane>
