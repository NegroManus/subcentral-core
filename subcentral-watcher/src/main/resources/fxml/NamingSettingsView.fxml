<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="rootPane" hgap="5.0"
	maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	minHeight="300.0" minWidth="500.0" vgap="5.0"
	xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
			minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity"
			vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" minHeight="-Infinity"
			vgrow="SOMETIMES" />
		<RowConstraints maxHeight="1.7976931348623157E308"
			minHeight="-Infinity" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Naming" underline="true">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../img/font_16.png" />
					</image>
				</ImageView>
			</graphic>
		</Label>
		<Label text="Naming parameters:" GridPane.rowIndex="1" />
		<TableView fx:id="namingParamsTableView" editable="true"
			maxHeight="1.7976931348623157E308" GridPane.rowIndex="2">
			<columns>
				<TableColumn fx:id="namingParamsNameColumn" editable="false"
					prefWidth="75.0" text="Name" />
				<TableColumn fx:id="namingParamsValueColumn" prefWidth="75.0"
					text="Value" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
